#
# Tests of natural ordering in "sort", "min", and "max"
#
sort($*)
min($*)
max($*)

a = [ ]
values = [ 10, 11, 1, 20, 2, 3, 40, 41, 30, 4, 5, 6, 7, 8, 9, 21, 12, 13, 14, 15, 16, 26, 42, 35, 39, 31, 32, 33 ]

:quiet on
loop over values { a[length(a)] = `$__` }
:quiet pop
a
sort(a)
sort(a, 4)
min(a)
max(a)

map = { a: 4, b:2, c:3, d: 1 }
sort map
sort map, 2

set = { 3, 4, 1, 2 }
sort set

map2 = { a: "Abc", f: "aBc", D: "abC", B: "ABC", c: "abc" }
sort map2
sort map2, 2
sort map2, 1
sort map2, 1|2
sort map2, 4

a9 = [ { name: 'joe' }, { name: 'benny' }, { name: 'abby' }, { name: 'diedre' } ]
sort a9

ar1 = [ "a1_1", "a_1", "a2_1" ]
ar2 = [ "a1-1", "a-1", "a2-1" ]
ar3 = [ "a1.1", "a.1", "a2.1" ]

sort ar1
sort ar2
sort ar3

