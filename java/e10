#
# Testing new looping construct
#
:echo "Tests of the new LOOP construct"
:echo

:clear [i, j, value, array, SIZE ]
:dec 50
value = 1
array = []
SIZE = 5
loop $i in SIZE {
   loop $j in SIZE {
      array[$i-1][$j-1] = value++!
   }
}
array;j

primes = []; numPrimes = 0;
start = 2; end = 102;
:quiet on
loop $i in start .. end { isprime($i) ? primes[numPrimes++] = $i : "" }
:quiet previous
numPrimes; primes;

:echo
:echo "Done with LOOP tests."

